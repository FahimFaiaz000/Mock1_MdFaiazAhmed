   "source": [
    "# 3) How do you filter rows of a DataFrame based on a condition?"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "a58da99b",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "   A  B\n",
      "2  3  3\n",
      "3  4  2\n",
      "4  5  1\n"
     ]
    }
   ],
   "source": [
    "import pandas as pd\n",
    "data = {\n",
    "    'A': [1, 2, 3, 4, 5],\n",
    "    'B': [5, 4, 3, 2, 1]\n",
    "}\n",
    "df = pd.DataFrame(data)\n",
    "filtered_df = df[df['A'] > 2]\n",
    "print(filtered_df)"
   ]
  }
